{
    "version": 2,
    "builds": [
        {
            "src": "quiz_gen_django/quiz_gen_django/wsgi.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "15mb",
                "runtime": "python3.10",
                "cwd": "quiz_gen_django"
            }
        },
        {
            "src": "build_files.sh",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "quiz_gen_django/staticfiles"
            }
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "/(.*)",
            "dest": "quiz_gen_django/quiz_gen_django/wsgi.py"
        }
    ]
}